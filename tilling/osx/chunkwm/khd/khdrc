# Launch Terminal
cmd - return                        :   open -na /Applications/iTerm.app

## Resize
cmd + shift - left 	: chunkc tiling::window --use-temporary-ratio 0.05 --adjust-window-edge west
cmd + shift - down 	: chunkc tiling::window --use-temporary-ratio 0.05 --adjust-window-edge south
cmd + shift - up 	: chunkc tiling::window --use-temporary-ratio 0.05 --adjust-window-edge north
cmd + shift - right 	: chunkc tiling::window --use-temporary-ratio 0.05 --adjust-window-edge east

lalt + shift - right 	: chunkc tiling::window --use-temporary-ratio -0.05 --adjust-window-edge west
lalt + shift - down 	: chunkc tiling::window --use-temporary-ratio -0.05 --adjust-window-edge south
lalt + shift - up 	: chunkc tiling::window --use-temporary-ratio -0.05 --adjust-window-edge north
lalt + shift - left 	: chunkc tiling::window --use-temporary-ratio -0.05 --adjust-window-edge east


## Swap position
cmd + lalt + shift - up     	:   chunkc tiling::window --swap north
cmd + lalt + shift - right  	:   chunkc tiling::window --swap east
cmd + lalt + shift - down   	:   chunkc tiling::window --swap south
cmd + lalt + shift - left   	:   chunkc tiling::window --swap west

## toggle split mode
cmd + shift - s 	: chunkc tiling::window --toggle toggle

## Toggle Float
cmd + shift - f 	: chunkc tiling::window --toggle native-fullscreen

## Mode
cmd + shift - v : chunkc tiling::desktop --layout bsp
cmd + shift - d : chunkc tiling::desktop --layout monocle

## Rotate
cmd + shift - r : chunkc tiling::desktop --rotate 90

lalt - w : khd -w "c4sofware@gmail.com"
